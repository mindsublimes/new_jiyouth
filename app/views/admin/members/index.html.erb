<div class="content-header">
  <div class="container-fluid">
    <div class="row mb-2">
      <div class="col-sm-6">
        <h1 class="m-0">User's</h1>
      </div>
      <!-- /.col -->
      <div class="col-sm-6">
        <ol class="breadcrumb float-sm-right">
          <li class="breadcrumb-item"><a href="/">Home</a></li>
          <li class="breadcrumb-item active">Dashboard v1</li>
        </ol>
      </div>
      <!-- /.col -->
    </div>
    <!-- /.row -->
  </div>
  <!-- /.container-fluid -->
</div>
<!-- /.content-header -->

<!-- Main content -->
<section class="content">
  <div class="container-fluid">
    <!-- /.row -->
    <!-- Main row -->

    <div class="row">
      <div class="col-12">
        <div class="card">
          <div class="card-header">
            <h3 class="card-title">Registered Members</h3>

            <div class="card-tools">
              <div class="input-group input-group-sm" style="width: 150px;">
                <input
                  type="text"
                  name="table_search"
                  class="form-control float-right"
                  placeholder="Search"
                />

                <div class="input-group-append">
                  <button type="submit" class="btn btn-default">
                    <i class="fas fa-search"></i>
                  </button>
                </div>
              </div>
            </div>
          </div>
          <!-- /.card-header -->
          <div class="card-body table-responsive p-0" style="height: 300px;">
            <table class="table table-head-fixed text-nowrap">
              <thead>
                <tr>
                  <th>#</th>
                  <th>Full Name</th>
                  <th>Father Name</th>
                  <th>CNIC #</th>
                  <th>Phone #</th>
                  <th>Email Address</th>
                  <th>Area</th>
                  <th>Tehsil Council</th>
                  <th>District</th>
                  <th>Province</th>
                </tr>
              </thead>
              <tbody>
                <% @members.each_with_index do |member,index| %>
                <tr>
                  <td><%= index + 1 %></td>
                  <td><%= member.name%></td>
                  <td><%= member.father_name%></td>
                  <td><%= member.cnic%></td>
                  <td><%= member.phone%></td>
                  <td><%= member.email%></td>
                  <td><%= member.area.title%></td>
                  <td><%= member.area.local_government_structure.title%></td>
                  <td>
                    <%= member.area.local_government_structure.district.title%>
                  </td>
                  <td>
                    <%=
                    member.area.local_government_structure.district.province.title%>
                  </td>
                </tr>
                <% end %>
                <tr>
                  <td>183</td>
                  <td>John Doe</td>
                  <td>11-7-2014</td>
                  <td><span class="tag tag-success">Approved</span></td>
                  <td>Bacon ipsum .</td>
                  <td>183</td>
                  <td>John Doe</td>
                  <td>11-7-2014</td>
                  <td><span class="tag tag-success">Approved</span></td>
                  <td>Bacon ipsum .</td>
                </tr>
              </tbody>
            </table>
          </div>
          <!-- /.card-body -->
        </div>
        <!-- /.card -->
      </div>
    </div>

    <!-- /.row (main row) -->
  </div>
  <!-- /.container-fluid -->
</section>
<!-- /.content -->
  